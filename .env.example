# Arogya Environment Variables Example
# Copy this file to .env and fill in your values

# API Configuration
API_BASE_URL=https://api.arogya.com
API_KEY=your_api_key_here

# Voice Assistant Configuration
VOICE_LANGUAGE=en-IN
VOICE_ENABLED=true

# Hospital Verification
HOSPITAL_API_URL=https://hospital-api.example.com
HOSPITAL_API_KEY=your_hospital_api_key

# Authentication
AUTH_PROVIDER=local
JWT_SECRET=your_jwt_secret_here

# PWA Configuration
PWA_ENABLED=true
OFFLINE_MODE=true

# Environment
NODE_ENV=development
PORT=3000
